@{
    ViewBag.Title = "Quản trị viên - Trang quản lý sản phẩm";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<div id="message"></div>

<div class="container"><button type="button" class="btn btn-sm btn-success pull-right mb-1" onclick="getProducts()">Thêm SP</button></div>

<table id="productTable" class="tableId table table-striped">
    <thead>
        <tr>  
            <th>Mã SP</th>
            <th>Tên SP</th>
            <th>Thương Hiệu</th>
            <th>Xuất sứ</th>
            <th>Thời Gian BH</th>
            <th>Giá</th>
            <th>Trạng thái</th>
            <th>Sửa</th>
            <th>Xóa</th>
            <th>Sửa trạng thái</th>
        </tr>
    </thead>
</table>

<div class="modal fade" role="dialog" id="productModal" aria-label="productModelLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content" id="productModalBody"></div>
    </div>
</div>


